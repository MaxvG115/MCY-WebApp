<p id="notice"><%= notice %></p>

<h1>Universitary Centers</h1>

<table class = "table">
  <thead>
    <tr>
      <th>Universitary center name</th>
      <th>Sede</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @universitary_centers.each do |universitary_center| %>
      <tr>
        <td><%= universitary_center.universitary_center_name %></td>
        <td><%= universitary_center.sede_id %></td>
        <td><%= link_to 'Show', universitary_center %></td>
        <td><%= link_to 'Edit', edit_universitary_center_path(universitary_center) %></td>
        <td><%= link_to 'Destroy', universitary_center, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Universitary Center', new_universitary_center_path %>

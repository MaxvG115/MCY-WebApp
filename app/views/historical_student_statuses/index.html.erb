<p id="notice"><%= notice %></p>

<h1>Historical Student Statuses</h1>

<table class = "table">
  <thead>
    <tr>
      <th>Studentstatus</th>
      <th>Alumnos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historical_student_statuses.each do |historical_student_status| %>
      <tr>
        <td><%= historical_student_status.studentstatus_id %></td>
        <td><%= historical_student_status.alumnos_id %></td>
        <td><%= link_to 'Show', historical_student_status %></td>
        <td><%= link_to 'Edit', edit_historical_student_status_path(historical_student_status) %></td>
        <td><%= link_to 'Destroy', historical_student_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Historical Student Status', new_historical_student_status_path %>

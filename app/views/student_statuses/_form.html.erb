<%= form_with model: student_statuses do |form| %>
  <div>
    <%= form.label :Nombre_estatus %><br>
    <%= form.text_field :status_name %>
    <% student_statuses.errors.full_messages_for(:status_name).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
<p id="notice"><%= notice %></p>

<h1>Sedes</h1>

<table class = "table">
  <thead>
    <tr>
      <th>Sede name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sedes.each do |sede| %>
      <tr>
        <td><%= sede.sede_name %></td>
        <td><%= link_to 'Show', sede %></td>
        <td><%= link_to 'Edit', edit_sede_path(sede) %></td>
        <td><%= link_to 'Destroy', sede, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h3>Importar Sedes</h3>
<%= form_tag import_sedes_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Subir sedes"%>
<%end%>

<%= link_to 'New Sede', new_sede_path %>

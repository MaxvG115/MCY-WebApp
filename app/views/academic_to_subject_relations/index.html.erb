<p id="notice"><%= notice %></p>

<h1>Academic To Subject Relations</h1>

<table>
  <thead>
    <tr>
      <th>Subjects</th>
      <th>Academicprograms idbody</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @academic_to_subject_relations.each do |academic_to_subject_relation| %>
      <tr>
        <td><%= academic_to_subject_relation.subjects_id %></td>
        <td><%= academic_to_subject_relation.academicprograms_idbody %></td>
        <td><%= link_to 'Show', academic_to_subject_relation %></td>
        <td><%= link_to 'Edit', edit_academic_to_subject_relation_path(academic_to_subject_relation) %></td>
        <td><%= link_to 'Destroy', academic_to_subject_relation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

